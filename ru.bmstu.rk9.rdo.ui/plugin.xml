        </markerResolutionGenerator>
        <markerResolutionGenerator
            class="ru.bmstu.rk9.rdo.ui.RDOExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
            markerType="ru.bmstu.rk9.rdo.ui.rdo.check.normal">
            <attribute
                name="FIXABLE_KEY"
                value="true">
            </attribute>
        </markerResolutionGenerator>
        <markerResolutionGenerator
            class="ru.bmstu.rk9.rdo.ui.RDOExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
            markerType="ru.bmstu.rk9.rdo.ui.rdo.check.expensive">
            <attribute
                name="FIXABLE_KEY"
                value="true">
            </attribute>
        </markerResolutionGenerator>
    </extension>
