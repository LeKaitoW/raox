  <extension
        point="org.eclipse.ui.handlers">
     <handler
           class="ru.bmstu.rk9.rdo.ui.RDOExecutableExtensionFactory:ru.bmstu.rk9.rdo.ui.runtime.BuildHandler"
           commandId="ru.bmstu.rk9.rdo.ui.runtime.build">
        <enabledWhen>
           <reference
                 definitionId="ru.bmstu.rk9.rdo.RDO.Editor.opened">
           </reference>
        </enabledWhen>
     </handler>
     <handler
           class="ru.bmstu.rk9.rdo.ui.RDOExecutableExtensionFactory:ru.bmstu.rk9.rdo.ui.runtime.ExecutionHandler"
           commandId="ru.bmstu.rk9.rdo.ui.runtime.execute">
        <enabledWhen>
           <and>
              <reference
                    definitionId="ru.bmstu.rk9.rdo.RDO.Editor.opened">
              </reference>
              <with
                    variable="ru.bmstu.rk9.rdo.ui.handlers.simulationRunning">
                 <equals
                       value="STOPPED">
                 </equals>
              </with>
           </and>
        </enabledWhen>
     </handler>
     <handler
           class="ru.bmstu.rk9.rdo.ui.RDOExecutableExtensionFactory:ru.bmstu.rk9.rdo.ui.runtime.StoppingHandler"
           commandId="ru.bmstu.rk9.rdo.ui.runtime.stop">
        <enabledWhen>
           <with
                 variable="ru.bmstu.rk9.rdo.ui.handlers.simulationRunning">
              <equals
                    value="RUNNING">
              </equals>
           </with>
        </enabledWhen>
     </handler>
  </extension>
