                <reference
                    definitionId="ru.bmstu.rk9.rao.Rao.Editor.opened">
                </reference>
            </activeWhen>
        </handler>
        <handler
            class="ru.bmstu.rk9.rao.ui.RaoExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
